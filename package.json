{
  "name": "nara-api",
  "version": "1.0.0",
  "description": "API para gerenciamento do NARA-BOT, incluindo conversas e mensagens.",
  "main": "dist/server.js",
  "packageManager": "yarn@4.5.1",
  "author": "Flavio Eduardo Tapparo",
  "license": "MIT",
  "scripts": {
    "build": "yarn tsc",
    "clean": "rm /dist -r -force",
    "start": "node dist/serverConsumer.js",
    "dev:api": "nodemon --watch src --ext ts,json --exec ts-node ./src/serverApi.ts",
    "dev:consumer": "nodemon --watch src --ext ts,json --exec ts-node ./src/serverConsumer.ts",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "amqplib": "^0.10.4",
    "axios": "^1.7.8",
    "cheerio": "^1.0.0",
    "cloudscraper": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "form-data": "^4.0.1",
    "fuse.js": "^7.0.0",
    "levenshtein": "^1.0.5",
    "pg": "^8.13.1",
    "puppeteer": "^23.8.0",
    "sharp": "^0.33.5",
    "string-similarity": "^4.0.4",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/levenshtein": "^1",
    "@types/pg": "^8.10.1",
    "@types/string-similarity": "^4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  },
  "opencv4nodejs": {
    "disableAutoBuild": true,
    "opencvIncludeDir": "C:/opencv/build/include",
    "opencvLibDir": "C:/opencv/build/x64/vc16/lib",
    "opencvBinDir": "C:/opencv/build/x64/vc16/bin",
    "opencvDir": "C:/opencv/build/x64/vc16"
  }
